@using HotelModels;
@model List<User>

@{int count = 1; }
<table class="table">
    <thead>
        <tr>
            <th scope="col">Num</th>
            <th scope="col">userName</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
        </tr>
    </thead>
    <tbody>
        @foreach (User u in Model)
        {
            <tr>
                <th scope="row">@count</th>
                <td>@u.UserName</td>
                <td>@u.FirstName</td>
                <td>@u.LastName</td>
            </tr>
            count++;
        }

    </tbody>
</table>






